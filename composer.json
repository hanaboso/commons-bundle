{
    "name": "hanaboso/commons-bundle",
    "description": "Hanaboso - commons-bundle",
    "license": "Apache-2.0",
    "version": "2.8.3",
    "require": {
        "php": "^8.1",
        "ext-apcu": "*",
        "ext-pdo": "*",
        "ext-sockets": "*",
        "ext-sodium": "*",
        "guzzlehttp/guzzle": "^7.5",
        "hanaboso/utils": "^1.2",
        "phpseclib/phpseclib": "^3.0",
        "windwalker/crypt": "^3.5"
    },
    "require-dev": {
        "aws/aws-sdk-php-symfony": "^2.5",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/mongodb-odm-bundle": "^4.5",
        "doctrine/orm": "^2.14",
        "hanaboso/php-check-utils": "^1.4",
        "php-imap/php-imap": "^4.5",
        "symfony/mailer": "^v6.2",
        "symfony/monolog-bundle": "^v3.8",
        "symfony/yaml": "^v6.2"
    },
    "suggest": {
        "aws/aws-sdk-php-symfony": "Install me, if you need to use FileStorage\\Driver\\Impl\\S3\\S3Driver.",
        "doctrine/doctrine-bundle": "Install me, if you using MySQL.",
        "doctrine/mongodb-odm-bundle": "Install me, if you using MongoDB.",
        "php-imap/php-imap": "Install me, if you need to use ImapConnector."
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\CommonsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CommonsBundleTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
