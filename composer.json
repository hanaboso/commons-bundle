{
    "name": "hanaboso/commons-bundle",
    "version": "2.2.2",
    "license": "Apache-2.0",
    "require": {
        "php": "^7.4",
        "ext-apcu": "*",
        "ext-sockets": "*",
        "guzzlehttp/guzzle": "^7.2",
        "hanaboso/utils": "^1.0",
        "phpseclib/phpseclib": "^2.0 || ^3.0",
        "windwalker/crypt": "^3.5"
    },
    "require-dev": {
        "ext-ftp": "*",
        "ext-soap": "*",
        "aws/aws-sdk-php-symfony": "^2.2",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/mongodb-odm-bundle": "^4.2",
        "doctrine/orm": "^2.8",
        "hanaboso/php-check-utils": "^1.1",
        "php-imap/php-imap": "^4.1",
        "symfony/mailer": "^5.2",
        "symfony/monolog-bundle": "^3.6",
        "symfony/yaml": "^5.2"
    },
    "suggest": {
        "aws/aws-sdk-php-symfony": "Install me, if you need to use FileStorage\\Driver\\Impl\\S3\\S3Driver.",
        "doctrine/doctrine-bundle": "Install me, if you using MySQL.",
        "doctrine/mongodb-odm-bundle": "Install me, if you using MongoDB.",
        "php-imap/php-imap": "Install me, if you need to use ImapConnector."
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\CommonsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CommonsBundleTests\\": "tests/"
        }
    }
}
